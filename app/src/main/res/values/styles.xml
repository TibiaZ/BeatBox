<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/red</item>
        <item name="colorPrimaryDark">@color/dark_red</item>
        <item name="android:colorAccent" tools:targetApi="lollipop">@color/gray</item>

        <item name="android:colorBackground">@color/shooting_blue</item>
        <item name="android:buttonStyle">@style/BeatBoxButton</item>
    </style>

    <!--Big nerd ranch style-->
    <style name="BeatBoxButton" parent="android:style/Widget.Holo.Button">
        <item name="android:background">@color/dark_blue</item>
    </style>


</resources>
